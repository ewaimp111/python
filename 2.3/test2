
def info(name,age,**other):
    print('name:'+name)
    print('age:'+str(age))
    print('other:'+str(other))

o ={'addr':'星湖路','city':'beijing'}
i=info('刘洋',22,**o)


def info1(name,*other):
    print(name)
    print(other)
    return other
n=[2,4,6]
y=[1,25,5]
x=('a','b')
f=info1('abc',n,y,x)
print(f[1])
